#include <iostream>
#include <conio.h>
#include <string>
#include <iomanip>

using namespace std;

float main()
{
	float mass; 
	float density;

	cout << "Mass in grams = "<< endl;
	cin >> mass;
	cout << "Density in grams per cubic centimeters = "<< endl;
	cin >> density;
	cout << "Volume = "<< mass / density  << " cm3" << endl;


	_getch();
	return 0;

}
