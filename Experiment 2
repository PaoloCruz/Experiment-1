#include <iostream>
#include <conio.h>
#include <string>
#include <iomanip>

using namespace std;

float main()
{
	float a; 
	float b;
	float c;
	

	cout << "Mass in grams = "<< endl;
	cin >> a;
	cout << "Density in grams per cubic centimeters = "<< endl;
	cin >> b;
	c = (b / a);
	cout << setprecision(3) << "Volume= " << c << " cm3"<< endl;


	_getch();
	return 0;

}
